#include <iostream>
using namespace std;

int main()
{
    string s,t;
    cin>>s>>t;

    string rev;

    for (int i = s.size() - 1; i >= 0; i--)
    {
        rev = rev + s[i];
    }

    if(t== rev)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }

    return 0;
}
