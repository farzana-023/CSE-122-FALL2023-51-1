#include<iostream>
using namespace std;

int main()
{
    int Total_Match;
    string Match;

    cin>>Total_Match;
    cin>>Match;

    int A=0;
    int D=0;

    for(int i=0;i<Total_Match;i++)
    {
        if(Match[i]=='A')
        {
            A++;
        }

        else
        {
            D++;
        }
    }

    if(A>D)
    {
        cout<<"Anton"<<endl;
    }
    else if(A<D)
    {
        cout<<"Danik"<<endl;
    }
    else
    {
        cout<<"Friendship"<<endl;
    }

    return 0;
}
